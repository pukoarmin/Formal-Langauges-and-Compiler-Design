BEGIN
    Integer n
    Boolean isPrime = True

    read(n)

    if n < 2:
        isPrime = False

    for index = 4, index < sqrt(n/2) + 1, index++:
        if n%i == 0:
            isPrime = False

    if isPrime == True:
        print("N is prime")
    else
        print("N is not prime")
END
